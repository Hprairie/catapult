ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-12/bin/nvcc

cflags = -pthread -B /home/prairie/miniforge3/envs/jit/compiler_compat -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /home/prairie/miniforge3/envs/jit/include -fPIC -O2 -isystem /home/prairie/miniforge3/envs/jit/include -fPIC -I/usr/local/cuda/include -I/usr/local/cuda-12/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/torch/csrc/api/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/TH -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/THC -I/usr/local/cuda-12/include -I/home/prairie/miniforge3/envs/jit/include/python3.11 -c
post_cflags = -O3 -std=c++17 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=saxpy_cuda -D_GLIBCXX_USE_CXX11_ABI=0
cuda_cflags = -I/usr/local/cuda/include -I/usr/local/cuda-12/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/torch/csrc/api/include -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/TH -I/home/prairie/miniforge3/envs/jit/lib/python3.11/site-packages/torch/include/THC -I/usr/local/cuda-12/include -I/home/prairie/miniforge3/envs/jit/include/python3.11 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 --use_fast_math -lineinfo -arch=sm_89 -std=c++17 --extended-lambda --expt-relaxed-constexpr -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=saxpy_cuda -D_GLIBCXX_USE_CXX11_ABI=0
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /home/prairie/Projects/catapult/cache/examples/build/temp.linux-x86_64-cpython-311/saxpy_torch.o: cuda_compile /home/prairie/Projects/catapult/cache/examples/saxpy_torch.cu






